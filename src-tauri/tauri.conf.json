{
  "productName": "tauri-app",
  "version": "0.0.0",
  "identifier": "com.notus.dev",
  "build": {
	"beforeDevCommand": "bun run dev",
	"devUrl": "http://localhost:1420",
	"beforeBuildCommand": "bun run build",
	"frontendDist": "../dist"
  },
  "app": {
	"windows": [
	  {
		  "title": "notus-app",
		"width": 1280,
		  "height": 720,
		  "decorations": false
	  }
	],
	"security": {
	  "csp": null
	}
  },
  "bundle": {
	"active": true,
	"targets": "all",
	"icon": [
	  "icons/32x32.png",
	  "icons/128x128.png",
	  "icons/128x128@2x.png",
	  "icons/icon.icns",
	  "icons/icon.ico"
	]
  }
}
